<%- include ../layouts/cabecera.ejs %>
<!-- //SE NECESITA PARA USERS -->
    <body >
        <script src="../../prueba/validacionDatos.js"> </script>
        <div id="preloader"></div>

        <section id="hero">
            <div class="hero-container">
                
                    <!--Envia la informacion a user y se recepciona el route/casas-->

                    <!-- datos de inicio de session  -->
                    <h1>inicio de sessión </h1>
                    <br>
                    <br>
                    <div class="form-group">
                        <label style="color:#ffffff" for="my-input"> electronico</label>
                        <input id="correo" class="form-control-file" type="text" name="correo" value="admin"
                            placeholder="Damo@example.com">
                    </div>
                    <div class="form-group">
                        <label style="color:#ffffff" for="my-input">contraseña</label>
                        <input id="contrasena" class="form-control-file" type="password" value="admin" name="contraseña">
                    </div>
                    <!-- b-btns -->
                    <% for( let i = 0; i < nicoDatosUsers.length; i++ ) { %>
                        <p style="color: red;"><%=nicoDatosUsers[i].id%>==
                            <%=nicoDatosUsers[i].nombre%>++<%=nicoDatosUsers[i].apellido%>
                            ==<%=nicoDatosUsers[i].edad%>==<%=nicoDatosUsers[i].correo%>
                        ==<%=nicoDatosUsers[i].contrasena%></p>
                        
                    <% } %> 
                    <br>
                    <center>
                        <button onclick="prueba()" >Coprobar datos</button>
                        <div id="impresion">

                        </div>
                        <a class="btn btn-primary" href="../casas">Cancelar </a>
                    </center>
              
            </div>
        </section>

        <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>
