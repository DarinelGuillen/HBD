                 <!-- <h1>length== <%= dataInmueblesRegistrados.length  %> </h1> -->
              </div>
        </div>
        


        <div class="row">

          <% for( let i = 0; i < dataInmueblesRegistrados.length; i++ ) { %>
              <% if (dataParaVerificadoUser[0].id==parseInt(dataInmueblesRegistrados[i].id_propietario)) { %>
                      <!--Se puede comprobar diponibilidad ademas -->
                      <div class="col-md-3">
                      <a class="portfolio-item" style="background-image: url('/images/<%=dataInmueblesRegistrados[i].img %>');">
                        <div class="details" style="color:rgb(5, 5, 5);">
                          <td> iterador i= <%= i %> ID=;; <%= dataInmueblesRegistrados[i].id %></td>
                          <br>
                          <b>ID propietario:<%=dataInmueblesRegistrados[i].id_propietario%>
                          </b>
                          <br>
                          <td>Dispoinibilidad:<%=dataInmueblesRegistrados[i].disponible1_No0%>
                          </td>
                          <br>
                          <td>Precio mensual:$<%=dataInmueblesRegistrados[i].precio%>
                          <br>
                          <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
                            <form class="form-inline" method="get" action="/casas/editar/<%=dataInmueblesRegistrados[i].id%> ">
                              <button class="btn btn-danger" type="submit">Editar</button>
                            </form>
                          </div>
      
                          <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
                            <form class="form-inline" method="post" action="/casas/eliminar/<%=dataInmueblesRegistrados[i].id%>">
                              <button class="btn btn-warning" type="submit">Borrar</button>
                            </form>
                          </div>
      
                        </div>
      
                      </a>
                    </div>
              <% }else{ %>
                  <div class="col-md-3">
                      <a class="portfolio-item" style="background-image: url('/images/<%=dataInmueblesRegistrados[i].img %>');">
                        <div class="details" style="color:rgb(5, 5, 5);">
                          <td> iterador i= <%= i %> ID=;; <%= dataInmueblesRegistrados[i].id %></td>
                          <br>
                          <b>ID propietario:<%=dataInmueblesRegistrados[i].id_propietario%>
                          </b>
                          <br>
                          <td>Dispoinibilidad:<%=dataInmueblesRegistrados[i].disponible1_No0%>
                          </td>
                          <br>
                          <td>Precio mensual:$<%=dataInmueblesRegistrados[i].precio%>
                          <br>
                  
                        </div>
      
                      </a>
                    </div>
                      
              <% } %>
      <% } %>
          
        </div>
        <div class="row">
          <center><h1>menu adm</h1></center> <br>
          <% if (dataParaVerificadoUser[0].admin==1) { %>
              <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
                  <form class="form-inline" method="get" action="/users/crearI/<%=dataParaVerificadoUser[0].id%>">
                    <button  class="btn btn-primary "type="submit">
                      <a  class="btn btn-primary " >Crear Un Nuevo Inmueble id=<%=dataParaVerificadoUser[0].id%></a>
                    </button>
                  </form><br>
              
                  <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
                    <form class="form-inline" method="get" action="/casas/editar/">
                      <button  class="btn btn-primary " style="color:rgb(233, 35, 17);" type="submit">Editar</button>
                    </form><br>
                  </div>

                      
              
                  <button type="button"  class="btn btn-primary "><a  class="btn btn-primary" href="/users/crearI">CREA UN NUEVO XXXXXXXXXXX </a></button><br>
              
                  <button type="button"  class="btn btn-primary " ><a  class="btn btn-primary "  href="/casas/crear/<%= dataParaVerificadoUser[0].correo %>,<%= dataParaVerificadoUser[0].contrasena%>">CREA UN NUEVO INMUEBLE </a></button>
  
          <% }else{ %>
                   <p>Puede imprimir algo si no admin </p>
                  
                      <% } %>


        </div>

        <div class="row">
          <center><h1>notificaciones</h1></center> <br>
              
              <% let num=0; %>
              <% for( let i = 0; i < dataNotifiUser.length; i++ ) { %>
              <% if (dataParaVerificadoUser[0].id==dataNotifiUser[i].id_propietario) { %>
                  <% for( let index = 0; index < dataInmueblesRegistrados.length; index++ ) { %>
                      <% if (dataInmueblesRegistrados[index].id==dataNotifiUser[i].id_inmueble) { %>
                              
                          

                          <p>
                              
                              El usuario con id <%= dataNotifiUser[i].id_user %><br>
                              le interesa el inmueble  en <%= dataInmueblesRegistrados[index].municipio %> .<br>
                               Id del inmueble <%= dataNotifiUser[i].id_inmueble %><br>
                               precio del inmueble <%= dataInmueblesRegistrados[index].precio %> <br>
                              <!--id_propietario <%= dataNotifiUser[i].id_propietario%> <br>-->
                              <img src='/images/<%=dataInmueblesRegistrados[index].img %>' alt="" width="200">
                           </p>
                       
                      <% } %>
                  <% } %>


              
              <% }else{%>
                  
              <% if (num==0) { %>
              <% num=1 %>
              <h1 style="color:red  ;">No tienes mas notificasiones por el momento</h1>
              <% } %>
              <%} %>
              <% } %>




          <table>
              <tr>
                  <td>hola</td>
              </tr>
              <tr>
                  <td>jeje</td>
              </tr>
          </table>

          
