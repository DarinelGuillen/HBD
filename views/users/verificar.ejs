          <p>
            id noti= <%= dataNotifiUser[i].id %> <br>
            id_user que envio la notificasion = <%= dataNotifiUser[i].id_user %> <br>
            mensaje de user= <%= dataNotifiUser[i].mensaje %> <br>
            id_propietario <%= dataNotifiUser[i].id_propietario%> <br>
            id_inmueble= <%= dataNotifiUser[i].id_inmueble %> <br>
          </p>
          <% }else{%>
            <% if (num==0) { %>
            <% num=1 %> 
              <h1 style="color:red  ;">No tienes mas notificasiones por el momento</h1>
                <% } %>
          <%} %>
   <% } %>
       
