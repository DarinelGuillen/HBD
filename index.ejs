        <a class="subscribe-btn" href="users/crear">Clic aquí</a>
      </div>
    </div>
  </div>
</section>

<!--==========================
  Visualización de inmueble
  ============================-->

<section id="portfolio">
  <div class="container wow fadeInUp">
    <div class="row">
      <div class="col-md-12">
        <h3 class="section-title">Visualización De Inmuebles </h3>
        <br>
        <center>
          <a href="/users/crear" class="button button-secondary button-pipaluk" type="button">Registrate Para Conocer Más</a>
        </center>
        <div class="section-title-divider"></div>
        <p class="section-description"></p>
      </div>
    </div>


    <div class="row">
      <% for( let i=0; i < data.length; i++ ) { %>
      <div class="col-md-3">
        <a class="portfolio-item" style="background-image: url('/images/<%=data[i].img %>');">
          <div class="details" style="color:rgb(0, 2, 2);">
            <br>
            <b>Municipio:<%=data[i].municipio%>
            </b>
            <br>
            <td>Precio mensual:$<%=data[i].precio%>
            </td>
            <br>
            <td>Dimenciones:<%=data[i].dimenciones%>m²
            </td>
            <br>
            <td>Codigo Postal:<%=data[i].codigo_postal%>
            </td>
            <br>

            <div class="btn-group-vertical" role="group" aria-label="Vertical button group">
              <form class="form-inline" method="" action="/users/crear">
                <button class="btn btn-danger" type="submit">Conocer Más </button>
              </form>
            </div>


          </div>

        </a>
      </div>

      <% } %>
    </div>
  </div>
</section>

